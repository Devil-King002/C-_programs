#include <iostream>
using namespace std;

int main() {
   
   char alphabet;
   cout << "Enter an alphabet: ";
   scanf("%c",&alphabet);
   
   alphabet = tolower(alphabet);
   
   if((alphabet>='a' && alphabet<='z')){
       switch(alphabet){
           case'a':
           case'e':
           case'i':
           case'u':
             cout << alphabet << " is a vowel" << endl;
             break;
           default:
             cout << alphabet << "is a consonant "<< endl;
       }
   }
   else{
       cout << "Invalid input. Please enter an alphabet. "<< endl;
   }
   

    return 0;
}
